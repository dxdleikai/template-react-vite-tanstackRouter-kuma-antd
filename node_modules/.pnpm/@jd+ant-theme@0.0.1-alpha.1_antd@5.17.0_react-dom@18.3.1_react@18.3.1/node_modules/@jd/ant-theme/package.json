{
  "name": "@jd/ant-theme",
  "version": "0.0.1-alpha.1",
  "description": "A react library developed with dumi",
  "license": "MIT",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "father build",
    "build:docs": "npm run docs:build && npm run upload",
    "build:watch": "father dev",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install && dumi setup",
    "prepublishOnly": "father doctor && npm run build",
    "start": "node ./node/start.js",
    "tailwind": "npx tailwindcss -i ./tailwind.css -o ./.dumi/tailwind.ignore.css",
    "tailwind-watch": "npx tailwindcss -i ./tailwind.css -o ./.dumi/tailwind.ignore.css --watch",
    "test": "yalc publish",
    "upload": "node ./node/upload.js"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@ant-design/compatible": "^5.1.2",
    "@ant-design/icons": "^5.2.6",
    "@ant-design/icons-svg": "^4.3.2",
    "@commitlint/cli": "^18.5.0",
    "@commitlint/config-conventional": "^18.5.0",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@jd/upload-oss-tools": "^1.1.23",
    "@types/lodash": "^4.14.202",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@umijs/lint": "^4.1.1",
    "ahooks": "^3.7.10",
    "antd": "^5.15.3",
    "autoprefixer": "^10.4.18",
    "colors": "^1.4.0",
    "concurrently": "^8.2.2",
    "copy-to-clipboard": "^3.3.3",
    "dumi": "^2.2.17",
    "eslint": "^8.56.0",
    "father": "^4.3.8",
    "husky": "^8.0.3",
    "js-beautify": "^1.14.11",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.9",
    "prism-react-renderer": "^2.3.1",
    "prism-themes": "^1.9.0",
    "re-resizable": "^6.9.11",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-sticky-box": "^2.0.5",
    "runes2": "^1.1.4",
    "stylelint": "^16.2.0",
    "tailwindcss": "^3.4.1"
  },
  "peerDependencies": {
    "antd": "^5.13.2",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "authors": [
    "yanhaowei@jd.com"
  ]
}
